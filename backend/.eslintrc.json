{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "google",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "max-len": ["error", {"code": 120}],
        "require-jsdoc": "off",
        "new-cap": "off",
        "valid-jsdoc": "off",
        "no-implicit-globals": "off",
        "no-unused-vars": "off",
        "no-invalid-this": "off",
        "no-undef": "off",
        "no-undef-init": "off",
        "quotes": ["error", "double"],
        "linebreak-style": ["error", "windows"],
        "object-curly-spacing": ["error", "always"],
        "indent": ["error", 4]
    }
}
